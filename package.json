{
  "name": "duosmium-results",
  "version": "1.0.0",
  "description": "A Science Olympiad tournament results archive.",
  "private": true,
  "author": "Duosmium Results (https://www.duosmium.org/results/)",
  "license": "MIT",
  "scripts": {
    "clean": "npm-run-all --parallel clean:*",
    "clean:site": "[ ! -d _site ] && mkdir _site; find _site ! -iwholename '*/data/*' ! -iwholename '*/images/*' -type f -delete",
    "clean:serverless": "find serverless ! -name 'index.js' -type f -delete; exit 0",
    "clean-windows": "rmdir /Q /S _site && rmdir /Q /S && mkdir _site",
    "format": "npx prettier --check .",
    "fix": "npx prettier --write .",
    "eleventy": "npx @11ty/eleventy",
    "watch:eleventy": "npm run eleventy -- --serve --incremental",
    "watch:assets": "NODE_ENV=development node esbuild.js",
    "build:eleventy": "ELEVENTY_PRODUCTION=true npm run eleventy",
    "build:assets": "node esbuild.js",
    "build": "npm-run-all clean --parallel build:*",
    "build-windows:eleventy": "set ELEVENTY_PRODUCTION=true & npm run eleventy",
    "build-windows:assets": "node esbuild.js",
    "build-windows": "npm-run-all clean-windows --parallel build-windows:*",
    "dev": "npm-run-all --parallel watch:*",
    "dev-windows": "npm-run-all --parallel watch:*",
    "netlify": "netlify dev -d _site -c 'npm run dev'"
  },
  "dependencies": {
    "@zip.js/zip.js": "^2.7.1",
    "bootstrap": "^4.6.2",
    "browserslist": "^4.21.5",
    "chartist": "^0.11.4",
    "chartist-plugin-axistitle": "^0.0.7",
    "daemonite-material": "^4.1.1",
    "instant.page": "^5.2.0",
    "jquery": "^3.6.4",
    "jspdf": "^2.5.1",
    "lodash": "^4.17.21",
    "mdb-ui-kit": "^3.11.0",
    "popper.js": "^1.14.7",
    "rfs": "^9.0.6",
    "sciolyff": "^0.11.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@netlify/functions": "^1.4.0",
    "@types/chroma-js": "^2.4.0",
    "@types/extract-colors": "^1.1.1",
    "autoprefixer": "^10.4.14",
    "chroma-js": "^2.4.2",
    "chrome-aws-lambda": "^10.1.0",
    "color-contrast-calc": "^0.4.1",
    "esbuild": "^0.17.15",
    "esbuild-sass-plugin": "^2.8.0",
    "html-minifier-terser": "^7.1.0",
    "js-yaml": "^4.1.0",
    "netlify-plugin-cache": "^1.0.3",
    "node-vibrant": "^3.2.1-alpha.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "sass": "^1.61.0",
    "simple-git": "^3.17.0"
  }
}
